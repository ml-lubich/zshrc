[run]
source = .
omit = 
    tests/*
    */tests/*
    */test_*
    setup.py
    conftest.py
    __pycache__
    .venv/*
    venv/*
    env/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

